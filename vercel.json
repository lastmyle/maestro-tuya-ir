{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "UV_SYSTEM_PYTHON": "1"
  },
  "regions": ["iad1"],
  "functions": {
    "main.py": {
      "maxDuration": 10
    }
  }
}
